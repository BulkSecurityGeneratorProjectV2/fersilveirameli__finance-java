<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd">
 <namespace>http://www.fsilveira.com.br/jsf/html</namespace>
 <!-- Componentes dos templates -->
 <tag>
  <tag-name>layout</tag-name>
  <source>/WEB-INF/templates/layout.xhtml</source>
 </tag>
 <tag>
  <tag-name>outputLabel</tag-name>
  <component>
   <component-type>br.com.fsilveira.OutputLabel</component-type>
   <handler-class/>
  </component>
 </tag>
 <tag>
  <tag-name>inputText</tag-name>
  <component>
   <component-type>br.com.fsilveira.InputText</component-type>
   <renderer-type>javax.faces.Text</renderer-type>
   <handler-class/>
  </component>
 </tag>
 <tag>
  <tag-name>inputFone</tag-name>
  <component>
   <component-type>br.com.fsilveira.InputFone</component-type>
   <renderer-type>javax.faces.Text</renderer-type>
   <handler-class/>
  </component>
 </tag>
 <tag>
  <tag-name>jquery</tag-name>
  <component>
   <component-type>br.com.fsilveira.JQuery</component-type>
   <renderer-type>javax.faces.Text</renderer-type>
   <handler-class/>
  </component>
 </tag>
 <tag>
  <tag-name>inputTime</tag-name>
  <component>
   <component-type>br.com.fsilveira.InputTime</component-type>
   <renderer-type>javax.faces.Text</renderer-type>
   <handler-class/>
  </component>
 </tag>
 <tag>
  <tag-name>inputNumeric</tag-name>
  <component>
   <component-type>br.com.fsilveira.InputNumeric</component-type>
   <renderer-type>javax.faces.Text</renderer-type>
   <handler-class/>
  </component>
 </tag>
 <tag>
  <tag-name>inputEmail</tag-name>
  <component>
   <component-type>br.com.fsilveira.InputEmail</component-type>
   <renderer-type>javax.faces.Text</renderer-type>
   <handler-class/>
  </component>
 </tag>
 <tag>
  <tag-name>selectOneMenu</tag-name>
  <component>
   <component-type>br.com.fsilveira.SelectOneMenu</component-type>
   <renderer-type>javax.faces.Menu</renderer-type>
   <handler-class/>
  </component>
  <attribute>
   <name>converter</name>
   <required>false</required>
   <type>javax.faces.convert.Converter</type>
  </attribute>
 </tag>
</facelet-taglib>
